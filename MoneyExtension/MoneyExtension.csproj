<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>BarbieriMoneyExtensions</PackageId>
    <Version>1.0.0</Version>
    <License>MIT</License>
    <Authors>Marcelo Barbieri</Authors>
    <Company>barbieri.dev</Company>
    <PackageDescripton>Extension to convert decimal to integer</PackageDescripton>
    <Description>Extension to convert decimal to integer</Description>
    <RepositoryUrl>https://github.com/marcelobarbieri/money-extensions</RepositoryUrl>
    <PackageProjectUrl>https://github.com/marcelobarbieri/money-extensions</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <PackageTags>money;int;decimal;converter</PackageTags>
    <Title>Money Extensions</Title>
    <Summary>Extension to convert decimal to integer</Summary>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Media\icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>
